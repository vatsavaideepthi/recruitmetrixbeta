@model List<UI.Models.timesheetobj>
@{
    ViewBag.Title = "Time Sheet";
    Layout = "~/Views/Shared/_JobPortalMasterView.cshtml";
    Layout = "~/Views/Shared/_loggedin.cshtml";
}

<span class="pageheader">Time Sheet</span>

<div id="requirementjobcollection" class="divjob" style="display: block;">
    <ul>
        @foreach (UI.Models.timesheetobj job in Model)
        {
          
            <li class="listRow jobrow"  data-jobid="" data-jobposteddate="" style="position:relative;padding:0px;">
               
                <div class="jobbox" style="padding:8px 2px;">
                    
                    <div style="width: 30%; margin: 0px 10px;" class="pull-left">
                        
                        <div style="margin-top: 3px; word-break: break-all">
                            <span class="jobskills">@job.data</span>
                        </div>
                    </div>

                    
                    

                    
                        
                    
                </div>
            </li>
        }
    </ul>
   
   
</div>
