@model UI.Models.timesheetobj
@{
    ViewBag.Title = "Time Sheet";
    Layout = "~/Views/Shared/_JobPortalMasterView.cshtml";
    Layout = "~/Views/Shared/_loggedin.cshtml";
}

<span class="pageheader">Time Sheet</span>

<div id="requirementjobcollection" class="divjob" style="display: block;">
    @if (Model != null)
    {
        
        <table class="table table-hover" style="border: 1px solid #ddd" id="delete">
            <thead>
                <tr>
                    <td><span class="">Date</span></td>
                    <td><span class="">Total Hours</span></td>
                    <td><span class="">Overtime Hours</span></td>
                    <td></td>
                </tr>
            </thead>
            <tbody>
                    <tr>
                         <td>
                            <div class="pull-left" style="padding: 2px 5px">
                                <span><span style="color: #abb1b8;" class="" data-timesheetid=""></span></span>
                            </div>

                        </td>
                        
                        <td>
                            <div class="pull-left" style="padding: 2px 5px">
                                <span><span style="color: #abb1b8;" class="" data-timesheetid="">@Model.metadata.totalhours</span></span>
                            </div>

                        </td>
                        <td>
                            <div class="pull-left" style="padding: 2px 5px">
                                <span><span style="color: #abb1b8;" class="" data-timesheetid="">@Model.metadata.overtimehours</span></span>
                            </div>

                        </td>
                        
                        
                    </tr>
            </tbody>
        </table>
 
    }

    else
    {<ul style="padding-top: 40px;">
        <li style="list-style: none;">
            <div class="well">
                <div class="pull-left" style="margin: 5px 0px 10px 0px; font-size: 25px; font-weight: bold;">
                    <span>No TimeSheets...</span>
                </div>

            </div>
        </li>
    </ul>
    }

</div>
