@{
    string highlighter = Convert.ToString(ViewBag.highlight);
    int value = 0;
    switch (highlighter)
    {
        case "jobs":
            value = 1;
            break;
        case "contacts":
            value = 2;
            break;
        case "mailinglists":
            value = 3;
            break;
        case "communication":
            value = 4;
            break;
        case "timesheets":
            value = 4;
            break;
    }
}
<div class="topbarmenu well" style="padding: 5px 0px 1px 0px; margin: 0px auto;background-color:#95a5a6;">
    <div class="container-fluid">
        <ul class="nav nav-pills levelone" style="margin: auto; display: inline-block;">
            
            <li class="@(value==1 ? "active" : "")" data-childmenuset="jobs">
                <a href="/jobs">Jobs</a>
            </li>
            <li class="@(value==2 ? "active" : "")" data-childmenuset="contacts">
                <a href="/contacts">Contacts</a>
            </li>
            <li class="@(value==3 ? "active" : "")" data-childmenuset="mailinglist">
                <a href="/lists">Mailing Lists</a>
            </li>
            <li class="@(value==4 ? "active" : "")" data-childmenuset="communication">
                <a href="/communication">Communication</a>
            </li>
             <li class="@(value==5 ? "active" : "")" data-childmenuset="timesheets">
                <a href="/timesheet">TimeSheets</a>
            </li>
        </ul>
    </div>
